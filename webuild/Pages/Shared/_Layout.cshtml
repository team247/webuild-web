<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - We build</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header class="container">
        <nav class="navbar navbar-expand-lg navbar-light">
            <nav class="navbar navbar-light">
                <a class="navbar-brand" href="#">
                    <img src="~/images/brand.svg" width="140" height="62" alt="" loading="lazy">
                </a>
            </nav>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav flex-grow-1 justify-content-center">
                    <li class="nav-item mx-4 active">
                        <a class="nav-link" href="#">MARKETPLACE <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item mx-4">
                        <a class="nav-link" href="#">WAYS TO INVEST</a>
                    </li>
                    <li class="nav-item mx-4">
                        <a class="nav-link" href="#">RESOURCES</a>
                    </li>
                </ul>
                <ul class="navbar-nav">
                    <li class="nav-item mx-4">
                        <a class="nav-link font-weight-bold" href="#">SIGN IN</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <main role="main">
        @RenderBody()
    </main>

    <footer>
        <div class="container overflow-hidden pt-4">
            <h2>WeBuild</h2>
            <p>299 Broadway,<br />New York,  New York</p>
            <p>
                <small>© 2020 WeBuild.  All rights reserved.</small>
            </p>
        </div>
    </footer>

    <script src="ttps://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="ttps://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="ttps://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
